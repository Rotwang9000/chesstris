{
	"name": "shaktris",
	"version": "1.0.0",
	"description": "A multiplayer game that combines elements of chess and Tetris",
	"main": "server.js",
	"scripts": {
		"start": "node server.js",
		"dev": "nodemon server.js",
		"test": "jest",
		"test:computer-players": "node tests/computer-players-test.js",
		"test:api": "node tests/api-endpoints-test.js",
		"test:api-simple": "node tests/api-test.js",
		"run:computer-players": "node scripts/run-computer-players.js",
		"run:callback-server": "node examples/callback-server.js",
		"run:simple-player": "node examples/simple-computer-player.js 5",
		"run:improved-player": "node examples/improved-computer-player.js 5",
		"setup:computer-player": "node scripts/setup-computer-player-env.js",
		"client:start": "cd client && react-scripts start",
		"client:build": "cd client && react-scripts build",
		"client:test": "cd client && react-scripts test",
		"client:eject": "cd client && react-scripts eject",
		"setup:client": "node scripts/setup-client.js",
		"build:client": "node scripts/build-client.js",
		"build": "npm run build:client",
		"clean": "rimraf client/build",
		"setup-and-start": "npm run setup:client && npm run dev"
	},
	"keywords": [
		"chess",
		"tetris",
		"game",
		"multiplayer",
		"socket.io",
		"three.js"
	],
	"author": "",
	"license": "MIT",
	"dependencies": {
		"axios": "^1.8.3",
		"bcryptjs": "^3.0.2",
		"body-parser": "^1.20.2",
		"cjs-loader": "^0.1.0",
		"crypto": "^1.0.1",
		"dotenv": "^16.4.7",
		"express": "^4.21.2",
		"jsonwebtoken": "^9.0.2",
		"minimist": "^1.2.8",
		"mongoose": "^8.12.1",
		"node-fetch": "^2.7.0",
		"prop-types": "^15.8.1",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-router-dom": "^6.30.0",
		"react-scripts": "^5.0.1",
		"redis": "^4.7.0",
		"sanitize-html": "^2.14.0",
		"sinon": "^19.0.2",
		"socket.io": "^4.8.1",
		"socket.io-client": "^4.8.1",
