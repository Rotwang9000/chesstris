		"@babel/preset-env": "^7.26.9",
		"@solana/web3.js": "^1.98.0",
		"@testing-library/jest-dom": "^5.17.0",
		"@testing-library/react": "^14.3.1",
		"@testing-library/user-event": "^14.6.1",
		"babel-jest": "^29.7.0",
		"chai": "^5.2.0",
		"jest": "^29.7.0",
		"nodemon": "^3.0.1",
		"web-vitals": "^3.5.2"
	},
	"engines": {
		"node": ">=14.0.0"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
